# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Yournal' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # FirebaseUI
  pod 'FirebaseUI/Auth', '~> 13.1'
  pod 'FirebaseUI/Email', '~> 13.1'
  pod 'FirebaseUI/Google', '~> 13.1'

  # Firebase
  pod 'Firebase/Auth', '~> 10.23'
  pod 'Firebase/Database', '~> 10.23'

  pod 'ProgressHUD', '~> 14.1'
  pod 'CustomIOSAlertView', '~> 0.9'
  pod 'MMMarkdown', '~> 0.5'
end
